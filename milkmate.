    <!-- Add JavaScript for the charts -->
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Toggle between sections
            const navLinks = document.querySelectorAll('.nav-link');
            const sections = document.querySelectorAll('section');
            
            navLinks.forEach(link => {
                link.addEventListener('click', function(e) {
                    e.preventDefault();
                    
                    const targetId = this.getAttribute('href').substring(1);
                    
                    sections.forEach(section => {
                        section.style.display = 'none';
                    });
                    
                    document.getElementById(targetId).style.display = 'block';
                    
                    navLinks.forEach(link => {
                        link.classList.remove('active');
                    });
                    
                    this.classList.add('active');
                });
            });
            
            // Revenue Chart (mockup only - would use Chart.js in production)
            const revenueCtx = document.getElementById('revenueChart');
            if (revenueCtx) {
                // For demonstration, this would be replaced with Chart.js
                const canvas = revenueCtx;
                const ctx = canvas.getContext('2d');
                
                // Mock chart - in production, use a proper charting library
                ctx.fillStyle = '#e3f2fd';
                ctx.fillRect(0, 50, 400, 150);
                
                ctx.strokeStyle = '#1565c0';
                ctx.lineWidth = 2;
                ctx.beginPath();
                ctx.moveTo(0, 150);
                ctx.lineTo(50, 120);
                ctx.lineTo(100, 140);
                ctx.lineTo(150, 80);
                ctx.lineTo(200, 100);
                ctx.lineTo(250, 70);
                ctx.lineTo(300, 50);
                ctx.lineTo(350, 30);
                ctx.lineTo(400, 50);
                ctx.stroke();
            }
            
            // Delivery Chart (mockup only - would use Chart.js in production)
            const deliveryCtx = document.getElementById('deliveryChart');
            if (deliveryCtx) {
                // For demonstration, this would be replaced with Chart.js
                const canvas = deliveryCtx;
                const ctx = canvas.getContext('2d');
                
                // Mock chart - in production, use a proper charting library
                ctx.fillStyle = '#e8f5e9';
                ctx.fillRect(0, 50, 400, 150);
                
                ctx.strokeStyle = '#2e7d32';
                ctx.lineWidth = 2;
                ctx.beginPath();
                ctx.moveTo(0, 100);
                ctx.lineTo(50, 110);
                ctx.lineTo(100, 90);
                ctx.lineTo(150, 85);
                ctx.lineTo(200, 95);
                ctx.lineTo(250, 80);
                ctx.lineTo(300, 70);
                ctx.lineTo(350, 60);
                ctx.lineTo(400, 50);
                ctx.stroke();
            }
        });
    </script>
